{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "moduleResolution": "node",
    "paths": {
      "@graphqlcqrs/core": [
        "libs/core/src"
      ],
      "@graphqlcqrs/core/*": [
        "libs/core/src/*"
      ],
      "@graphqlcqrs/common": [
        "libs/common/src"
      ],
      "@graphqlcqrs/common/*": [
        "libs/common/src/*"
      ],
      "@graphqlcqrs/repository": [
        "libs/repository/src"
      ],
      "@graphqlcqrs/repository/*": [
        "libs/repository/src/*"
      ],
      "@juicycleff/nest-multi-tenant": [
        "libs/nest-multi-tenant/src"
      ],
      "@juicycleff/nest-multi-tenant/*": [
        "libs/nest-multi-tenant/src/*"
      ],
      "@ultimatebackend/contracts": [
        "libs/contracts/src"
      ],
      "@ultimatebackend/contracts/*": [
        "libs/contracts/src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "files": [
    "definitions/type.d.ts"
  ]
}
