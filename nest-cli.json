{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway-admin/src",
  "monorepo": true,
  "root": "apps/gateway-admin",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/gateway-admin/tsconfig.app.json",
    "assets": [
      { "include": "**/*.graphql" },
      { "include": "config/*.yml" }
    ]
  },
  "projects": {
    "gateway-admin": {
      "type": "application",
      "root": "apps/gateway-admin",
      "entryFile": "main",
      "sourceRoot": "apps/gateway-admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway-admin/tsconfig.app.json"
      }
    },
    "service-user": {
