{
  "name": "ultimate-backend",
  "version": "0.0.2-alpha2",
  "description": "This is an enterprise scale advanced microservice pattern with GraphQL",
  "author": {
    "name": "Rex Isaac Raphael",
    "email": "rex.rahael@outlook.com",
    "url": "https://xraph.com"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/gateway-admin/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint-md": "remark .",
    "test": "snyk test && jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/gateway-admin/test/jest-e2e.json",
    "kompose": "kompose convert -o deployment.yaml",
    "docs": "npx compodoc --theme material --output docs -p tsconfig.json -s",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "dependencies": {
    "@apollo/gateway": "^0.10.6",
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/cqrs": "^6.0.1",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.2.0",
    "@types/continuation-local-storage": "^3.2.2",
    "@types/express-session": "^1.15.15",
    "apollo-server": "^2.9.6",
    "apollo-server-express": "^2.9.5",
    "aws-sdk": "^2.546.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "cache-manager": "^2.10.0",
    "casbin": "^3.0.4",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.2",
    "connect-mongodb-session": "^2.2.0",
    "continuation-local-storage": "^3.2.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.1.0",
    "express-session": "^1.17.0",
    "fastify-swagger": "^2.4.0",
    "geteventstore-promise2": "^3.1.2",
    "graphql": "^14.5.8",
    "graphql-passport": "^0.4.0",
    "graphql-tools": "^4.0.5",
    "greenlock-express": "2.x",
    "greenlock-store-fs": "3",
    "influx": "^5.4.2",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.15",
    "mongo-cursor-pagination": "^7.1.0",
    "mongodb": "^3.3.3",
    "node-eventstore-client": "^0.2.13",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "request-context": "^2.0.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "set-cookie-parser": "^2.4.0",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.19",
    "typeorm-adapter": "^1.0.4",
    "typeorm-linq-repository": "^1.0.1",
    "typeorm-transactional-cls-hooked": "^0.1.8",
    "ursa-optional": "^0.10.1",
    "uuid": "^3.3.3",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.144",
    "@types/mongodb": "^3.3.6",
    "@types/node": "^12.7.5",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "remark-cli": "^7.0.0",
    "remark-lint-emphasis-marker": "^1.0.3",
    "remark-lint-strong-marker": "^1.0.3",
    "remark-preset-lint-recommended": "^3.0.3",
    "snyk": "^1.236.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@graphqlcqrs/core/(.*)": "<rootDir>/libs/core/src/$1",
      "@graphqlcqrs/graphql-gateway/(.*)": "<rootDir>/libs/graphql-gateway/src/$1",
      "@graphqlcqrs/nestjs-event-store/(.*)": "<rootDir>/libs/nestjs-event-store/src/$1",
      "@graphqlcqrs/common/(.*)": "<rootDir>/libs/common/src/$1",
      "@juicycleff/nestjs-influxdb/(.*)": "<rootDir>/libs/nestjs-influxdb/src/$1",
      "@graphqlcqrs/repository/(.*)": "<rootDir>/libs/repository/src/$1",
      "@graphqlcqrs/nest-casbin/(.*)": "<rootDir>/libs/nest-casbin/src/$1",
      "@juicycleff/nest-multi-tenant/(.*)": "<rootDir>/libs/nest-multi-tenant/src/$1"
    }
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
